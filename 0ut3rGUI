e1x1x1x1Message.MouseButton1Down:connect(function()
    local hint = true
    function createHint()
      if hint == true then
        local h = Instance.new("Hint", Workspace)
        while true do
          wait(4)
          h.Text = "0ut3r is amazing"
          wait(4)
          h.Text = "0ut3r is the best"
          wait(4)
          h.Text = "niggas watch from far away his success"
          wait(4)
          h.Text = "bro fr nuking these servers"
          wait(4)
          h.Text = "Anyways, HACKED BY 0UT3RKIDD BITCHEZ!!!"
        end
      end
    end
    createHint()
end)
