      wait(3)
      for i, c in pairs(game.Players:GetChildren()) do
        local alertGUI = Instance.new("ScreenGui", c.PlayerGui)
        alertGUI.Name = "alert"
        local image = Instance.new("ImageLabel", alertGUI)
        image.Name = "img"
        image.Image = "rbxassetid://16784629494"
        image.Size = UDim2.new(10, 0, 20, 0)
